{
  "name": "@jahed/webpack-html-meta",
  "version": "1.0.0",
  "description": "Meta and Favicon Generator for Webpack",
  "author": "Jahed Ahmed",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "echo 'No tests yet :('",
    "prepublishOnly": "yarn test && yarn run build",
    "postpublish": "git push && git push --tags",
    "docs": "bash -i ./docs.sh"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.2",
    "debug": "3.1.0",
    "favicons": "5.1.1",
    "lodash": "4.17.10"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.7",
    "@types/debug": "0.0.30",
    "@types/html-webpack-plugin": "2.30.3",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.108",
    "@types/node": "8.10.10",
    "@types/tapable": "1.0.2",
    "@types/webpack": "4.1.4",
    "jest": "22.4.3",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.4",
    "typedoc": "0.11.1",
    "typescript": "2.8.3",
    "webpack": "4.6.0"
  },
  "peerDependencies": {
    "html-webpack-plugin": ">=3",
    "webpack": ">=4"
  }
}
